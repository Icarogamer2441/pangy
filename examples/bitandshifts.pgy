class Main {
    def main() -> void {
        var a int = 10
        var b int = 20

        print("a & b = ", a & b)
        print("a | b = ", a | b)
        print("a ^ b = ", a ^ b)
        print("~a = ", ~a)
        print("a << 1 = ", a << 1)
        print("a >> 1 = ", a >> 1)
        print("a >>> 1 = ", a >>> 1)
        print("a << 2 = ", a << 2)
        print("a >> 2 = ", a >> 2)
        print("a >>> 2 = ", a >>> 2)
    }
}