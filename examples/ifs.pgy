class Main {
    def main() -> void {
        this:test(10)
        this:test(20)
        if (0 < 1) {
            print("0 is less than 1")
        } else {
            print("0 is greater than 1")
        }
    }

    def test(a int) -> void {
        if (a > 10) {
            print("a is greater than 10")
        } else if (a < 10) {
            print("a is less than 10")
        } else {
            print("a is equal to 10")
        }
    }
}